// colors
$black = #000
$white = #fff
$yellow = #ffdd15
$grey = #666

// dimensions
$page-width = 100%;

$footer-height = 150px;
$header-height = 50px;

// z-indices
zIndices = {
  "negative": -1,
  "base": 0,
  "increment": 1,
  "jump": 10,
  "floating-menu": 1000,
  "lightbox": 10000,
}

zIndex(id)
  z-index: zIndices[id]
